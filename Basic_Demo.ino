int get_value(int *arrayhuh,int x,int y,int multiple){
  return *(arrayhuh +(multiple * x+y));
}
void change_value(int *arrayhuh,int x,int y, int multiple,int value){
  *(arrayhuh +(multiple * x+y)) = value;
}

int sales(int *arrayhuh, int *nope, int lengthi, int multiple){
  Serial.println("Start");
  // haha is the array that we're going to test
  int current_position_length = 0;
  // current_position_length is for nope
  // nope is the array that we're going twice as large as haha
  int current_position_subarray_length = 0;
  // current_position_subarray_length is for nope
  int haha_subarray_length = lengthi;
  int haha_length = lengthi;
  int nope_length = lengthi * multiple;

  Serial.println("Copy array");
  for(int i = 0;i < haha_length;i++){
    current_position_subarray_length = 0;
    for(int j = 0;j < haha_subarray_length;j++){
      for(int g = 0; g < multiple;g++){
        change_value(nope,current_position_length,current_position_subarray_length + g,nope_length,get_value(arrayhuh,i,j,lengthi));
      }
      //copying so that it would be twice as long
      current_position_subarray_length = current_position_subarray_length + multiple;
    }
    for(int l = 0;l < haha_subarray_length * multiple;l++){
      for(int p = 0;p < multiple;p++){
        change_value(nope,current_position_length + p,l,nope_length,get_value(nope,current_position_length,l,nope_length));
      }
    }
    //copying the previous line of array
    current_position_length = current_position_length + multiple;
    //increase current_position_length by 2
  }
  
  Serial.println("Dump array\n");
  for(int k = 0; k < nope_length; k++){
    for(int j = 0; j < nope_length; j++){
      if(get_value(nope, k, j, nope_length) == 0){
        tft.drawPixel(k,j, COLOR_WHITE);
      }
      else if(get_value(nope, k, j, nope_length) == 1){
        tft.drawPixel(k,j, COLOR_BLACK);
      }
    }
  }
  return 0;
}



